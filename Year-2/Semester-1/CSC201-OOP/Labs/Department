public class Department {

    private String [] students;
    private String depart;

public Department(String []students , String department ){
     this.students = students;
     this.depart=department;
}

    public  void print(){

        System.out.println("Department Name : "+depart);

        for(String x: students) {
            System.out.println("Student Name : "+x);
        }
    }

    public static void main(String [] args){

        String [] students = {"Ali","Jassim","Sara"};

        Department obj = new Department(students,"Cs");

        obj.print();

    }

}
